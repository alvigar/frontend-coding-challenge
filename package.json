{
  "name": "react-typescript",
  "version": "1.0.0",
  "description": "React and TypeScript example starter project",
  "keywords": [
    "typescript",
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mdi/js": "6.5.95",
    "@mdi/react": "1.5.0",
    "@mui/material": "5.4.2",
    "@mui/styles": "5.4.2",
    "@types/lodash": "4.14.178",
    "i18next": "21.6.11",
    "lodash": "4.17.21",
    "mobx": "6.3.13",
    "mobx-react": "7.2.1",
    "notistack": "2.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "11.15.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "^5.3.3",
    "cypress": "^15.8.1",
    "react-error-overlay": "^6.0.9",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/**/*.d.ts",
      "!src/setupTests.ts"
    ]
  }
}